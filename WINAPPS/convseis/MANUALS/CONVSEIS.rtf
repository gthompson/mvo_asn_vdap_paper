\par }\pard \ql \li1134\ri49\widctlpar\aspalpha\aspnum\faauto\adjustright\rin49\lin1134\itap0 {\f2\fs20 RENCHGSE v1.1 - GSE utility program                      
\par 
\par COMMAND MODE:                                            
